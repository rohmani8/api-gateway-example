{
    "name": "DEV RED ERP - API Gateway",
    "host": "http://192.168.10.194:8081",
    "port": 8080,
    "extra_config":{
        "telemetry/logging": {
          "level": "DEBUG",
          "prefix": "[KRAKEND]",
          "syslog": false,
          "stdout": true
        },
        "auth/validator": {
          "@comment": "JWK shared cache amongst all endpoints of 15 minutes",
          "shared_cache_duration": 900
        },
        "router": {
          "auto_options": true,
          "return_error_msg": true
        },
        "security/cors": {
          "allow_origins": ["*"],
          "allow_headers": ["*"],
          "allow_methods": ["GET","HEAD","POST","PUT","DELETE"],
          "max_age": "12h",
          "allow_credentials": false,
          "debug": false
        }
    }
}